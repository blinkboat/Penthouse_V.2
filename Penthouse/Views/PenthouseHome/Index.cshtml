@{
    ViewData["Title"] = "Thailand Penthouse";

}

<!-- START AGENCY-HOME -->



<section class="section agency-home" id="home">
    <div class="bg-overlay">
        <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
            <ol class="carousel-indicators">
                <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                @*<li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="4"></li>*@
            </ol>
            <div class="carousel-inner" role="listbox">
                <!-- Slide One - Set the background image for this slide in the line below -->
                @{
                    var rowindex = 0;
                    foreach (var item in Model)
                    {
                        if (rowindex == 0)
                        {
                            <div class="carousel-item active" style="background-image:url(@item.image_url)">
                                <div class="home-center">
                                    <div class="home-desc-center">
                                        <div class="container">
                                            <div class="row">
                                                <div class="col-lg-12">
                                                    <div class="agency-home-content text-center  text-white">
                                                        <p class="agency-home-sub-title text-uppercase f-14 letter-spacing_4">@item.province</p>
                                                        <h4 class="agency-home-title text-uppercase mt-4 letter-spacing_6">@item.projectname</h4>
                                                        <div class="mt-5">
                                                            <a href="@item.link" target="_blank" class="btn btn-custom">ABOUT PROJECT</a>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }
                        else
                        {
                            <div class="carousel-item" style="background-image:url(@item.image_url)">
                                <div class="home-center">
                                    <div class="home-desc-center">
                                        <div class="container">
                                            <div class="row">
                                                <div class="col-lg-12">
                                                    <div class="agency-home-content text-center  text-white">
                                                        <p class="agency-home-sub-title text-uppercase f-14 letter-spacing_4">@item.province</p>
                                                        <h4 class="agency-home-title text-uppercase mt-4 letter-spacing_6">@item.projectname</h4>
                                                        <div class="mt-5">
                                                            <a href="@item.link" target="_blank" class="btn btn-custom">ABOUT PROJECT</a>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }
                        rowindex = 1;

                    }
                }


                <!-- Slide Two - Set the background image for this slide in the line below -->
                @*<div class="carousel-item" style="background-image:url(../../images/img_Project/MThonglor10/Materials-for-TP-M-Thonglor-Homepage.jpg)">

            <div class="home-center">
                <div class="home-desc-center">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="agency-home-content text-center  text-white">
                                    <p class="agency-home-sub-title text-uppercase f-14 letter-spacing_4">BANGKOK</p>
                                    <h4 class="agency-home-title text-uppercase mt-4 letter-spacing_6">M Thonglor 10</h4>

                                    <div class="mt-5">
                                        <a href="~/ProjectDetails/MThonglor10" target="_blank" class="btn btn-custom">ABOUT PROJECT</a>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <div class="carousel-item" style="background-image:url(../../images/img_Project/TheMonumentThonglor/Materials-for-TP-THMNMTL-Homepage1.jpg)">

            <div class="home-center">
                <div class="home-desc-center">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="agency-home-content text-center  text-white">
                                    <p class="agency-home-sub-title text-uppercase f-14 letter-spacing_4">BANGKOK</p>
                                    <h4 class="agency-home-title text-uppercase mt-4 letter-spacing_6">Monument Thonglor</h4>

                                    <div class="mt-5">
                                        <a href="~/ProjectDetails/MONUMENTTHONGLO" target="_blank" class="btn btn-custom">ABOUT PROJECT</a>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>*@
                <!-- Slide Three - Set the background image for this slide in the line below -->

            </div>
            <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div>

</section>
<section class="section" id="about">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-8">
                <div class="blog-post-box text-center mt-3">
                    <div class="post-standard-title">
                        <div class="post-img mt-3">
                            <a href="#">
                                <img src="~/images/AboutUS/Materials-for-TPH-About-Us.jpg" class="img-fluid" alt="">
                            </a>
                        </div>
                        <div class="blog-standard-content mt-5">
                            <div class="home-title text-center">
                                <h2 class="home-title-title letter-spacing_4 text-uppercase">ABOUT US</h2>
                                <p class="text-muted mt-4 textcenter">
                                    Thailandpenthouses.com is the only website which collects luxury property in Thailand under Bridge Estate (Thailand), that carefully selected penthouses listed from
                                    around country with thoughts of hope to assist and make it easier for buyers and investors around the world.
                                </p>
                                <p class="text-muted mt-4 textcenter">
                                    Premium penthouses around country have been exclusively selected by us to make sure that you will get the best project with the best location that suits you.
                                </p>
                                <p class="text-muted mt-4 textcenter">
                                    For Bridge Estate, we are an experienced international real estate agent in Thailand that focus exclusively on luxury residential real estate and proud to help you
                                    choosing your perfect property in Thailand.
                                </p>
                            </div>
                        </div>

                    </div>
                </div>

            </div>
        </div>


    </div>
</section>


